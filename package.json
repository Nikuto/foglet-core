{
  "name": "foglet-core",
  "version": "4.1.0",
  "description": "Core of the foglet library",
  "main": "./foglet-core.js",
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "fix": "standard --fix",
    "esdoc": "esdoc",
    "all": "npm run build && npm run min && npm test && npm run esdoc",
    "test": "node ./tests/test.js",
    "test-bis": "foglet-scripts test -- --karma ./foglet-karma-config.js",
    "replace": "node tests/replacesignaling.js",
    "start": "foglet-scripts start",
    "build": "foglet-scripts build",
    "min": "webpack --config ./foglet-webpack-config-min.js",
    "example": "npm run build && node tests/examples/server.js",
    "dev": "webpack --config ./foglet-webpack-config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ran3d/foglet-core.git"
  },
  "keywords": [
    "foglet",
    "spray-wrtc",
    "browser-to-browser",
    "webRTC",
    "foglet computing"
  ],
  "author": "Folkvir",
  "contributors": [
    "Chat Wane (https://github.com/Chat-Wane)",
    "Thomas Minier (https://github.com/Callidon)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ran3d/foglet-core/issues"
  },
  "homepage": "https://github.com/ran3d/foglet-core#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "lodash.merge": "^4.6.1",
    "lodash.sortedindexby": "^4.6.0",
    "n2n-overlay-wrtc": "^2.0.9",
    "readable-stream": "^2.3.6",
    "socket.io-client": "^2.1.1",
    "spray-wrtc": "^2.0.x",
    "tman-wrtc": "0.1.x",
    "unicast-definition": "^2.0.1",
    "uuid": "^3.2.1",
    "version-vector-with-exceptions": "^0.1.0"
  },
  "devDependencies": {
    "cors": "^2.8.4",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-node": "^1.0.3",
    "esdoc-standard-plugin": "^1.0.0",
    "foglet-scripts": "^0.3.x",
    "foglet-signaling-server": "^1.0.0",
    "jquery": "^3.3.1",
    "karma-firefox-launcher": "^1.1.0",
    "p2p-graph": "^1.2.2",
    "snazzy": "^7.1.1",
    "twilio": "^3.17.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack-cli": "^2.1.3"
  },
  "bundledDependencies": [
    "debug",
    "lodash.merge",
    "lodash.sortedindexby",
    "readable-stream",
    "socket.io-client",
    "spray-wrtc",
    "tman-wrtc",
    "unicast-definition",
    "uuid",
    "version-vector-with-exceptions"
  ],
  "standard": {
    "globals": [
      "describe",
      "it",
      "assert"
    ]
  }
}
